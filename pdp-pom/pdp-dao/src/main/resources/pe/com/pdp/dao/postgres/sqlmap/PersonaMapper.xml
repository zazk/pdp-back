    where "id" = #{id,jdbcType=INTEGER}
  </update>
  
  
  
     <!-- QUERIES ADICIONALES -->
  	<select id="selectByPersona"  resultMap="ResultPersona" parameterType="HashMap">
  
	  	SELECT 
	  		pers.id ,  
	  		pers.descripcion ,  
	  		pers.tipo_documento , 
            pers.nro_documento ,  
            pers.nombres ,  
            pers.apellido_materno , 
            pers.apellido_paterno ,  
            pers.genero ,  
            pers.nombre_completo , 
            pers.estado ,  
            pers.ubicacion_catastro ,  
            pers.tipo_persona , 
            pers.fecha_nacimiento,
            email.email,
            elemento1.des_nombre as des_tipo_documento,
  		    elemento2.des_nombre as des_tipo_persona
           
		  FROM "pataPerro".tm_persona pers
		  inner join "pataPerro".tm_email email on
		  email.persona_id=pers.id
		  
		  inner join "pataPerro".tm_elemento elemento1 on
		  elemento1.id=pers.tipo_documento
		  inner join "pataPerro".tm_elemento elemento2 on
		  elemento2.id=pers.tipo_persona

	       
	     where
	     pers.estado=1 and email.estado=1 and email.ind_principal=1
	     
	    <if test="PERSONA_ID != null">
	    	and pers.id= ${PERSONA_ID}
	 	 </if>
	 	 
	 	 
	 	 <if test="NOMBRE_COMPLETO!= null">
	 		and UPPER(pers.nombre_completo) like '%${NOMBRE_COMPLETO}%'
	 	 </if>
	 	 
	 	 <if test="TIPO_DOCUMENTO != null">
	 	 	and pers.tipo_documento=${TIPO_DOCUMENTO}
	 	 </if>
	 	 
	 	  <if test="NRO_DOCUMENTO != null">
	 	 	and pers.nro_documento like '%${NRO_DOCUMENTO}%'
	 	 </if>
	 	 
	 	 
	 	 <if test="TIPO_PERSONA != null">
	 		 and pers.tipo_persona=${TIPO_DOCUMENTO}
	 	 </if>
	 	 
	 	 	order by 1 asc
	 	 	
	 	 <if test="INICIO_PAG != null">
	 	   LIMIT 10 OFFSET ${INICIO_PAG};
	 	 </if>
	 
  
   </select>
   
