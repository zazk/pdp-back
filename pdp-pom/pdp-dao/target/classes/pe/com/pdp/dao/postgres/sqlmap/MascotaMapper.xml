			LEFT JOIN "pataPerro".td_persona_mascota pm
			  ON ma."id" = pm."mascota_id"
			LEFT JOIN "pataPerro".tm_persona pe
			  ON pe."id" = pm."persona_id"
			LEFT JOIN "pataPerro".tm_elemento mr
			  ON ma."raza" = mr."id"
			LEFT JOIN "pataPerro".tm_elemento ms
			  ON ma."genero" = ms."id"
			LEFT JOIN "pataPerro".tm_elemento mt
			  ON ma."tamanio" = mt."id"
			LEFT JOIN "pataPerro".tm_elemento mc
			  ON ma."color" = mc."id"  
			
			WHERE
				  ma."estado" = 1
				
				  <if test="id != null">
			        AND ma."id" = #{id}
			      </if>	
				
				  <if test="personaId != null">
			        AND pm."persona_id" = #{personaId}
			      </if>
			      
			      order by ma."fecha_registro" asc
			
  </select>
  

  	<select id="selectByMascota"  resultMap="ResultMascota" parameterType="HashMap">
  
	  	SELECT 
  		   mascota.id,
  		   mascota.nombre,
  		   mascota.genero, 
           genero.des_abreviacion as des_genero,
      	   mascota.tamanio,
           tamanio.des_nombre as des_tamanio,
      	   mascota.raza,
           raza.des_nombre as des_raza,
      	   mascota.distrito, 
      	   mascota.caracteristica,
      	   mascota.ind_recibido, 
      	   mascota.des_comentario, 
           mascota.ubicacion_catastro,
           mascota.color,
           color.des_nombre as des_color, 
           mascota.estado,
           persona.tipo_documento,
           persona.nro_documento,
           persona.nombres,
           persona.apellido_paterno,
           persona.apellido_materno,
           persona.nombre_completo,
           persona.tipo_persona,
           tipo_persona.des_nombre as des_tipo_persona
           
  		      
		  FROM "pataPerro".tm_mascota mascota
		  
		  inner join "pataPerro".td_persona_mascota per_mascot on
		  mascota.id=per_mascot.mascota_id
		  
		  inner join  "pataPerro".tm_persona persona on
		  persona.id=per_mascot.persona_id
          
          inner join  "pataPerro".tm_elemento genero on
          genero.id=mascota.genero
          
          inner join  "pataPerro".tm_elemento tamanio on
          tamanio.id=mascota.tamanio
          
          inner join  "pataPerro".tm_elemento raza on
          raza.id=mascota.raza
          
          inner join  "pataPerro".tm_elemento color on
          color.id=mascota.color
          
          inner join  "pataPerro".tm_elemento tipo_persona on
          tipo_persona.id=persona.tipo_persona
           
          
          left join "pataPerro".tm_ubigeo ubigeo on
          ubigeo.id=mascota.distrito
          
	       
	     where
	
	   	mascota.estado=1
	     
	    <if test="MASCOTA_ID != null">
	    	and mascota.id= ${MASCOTA_ID}
	 	 </if>
	 	 
	 	 <if test="MASCOTA_NOMBRE != null">
	 		and UPPER(mascota.nombre) like '%${MASCOTA_NOMBRE}%'
	 	 </if>
	 	 
	 	 <if test="TIPO_RAZA != null">
	 	 	and mascota.raza=${TIPO_RAZA}
	 	 </if>
	 	 
	 	  <if test="TIPO_TAMANIO != null">
	 	 	and mascota.tamanio=${TIPO_TAMANIO}
	 	 </if>
	 	 
	 	  <if test="TIPO_GENERO != null">
	 	 	and mascota.genero=${TIPO_GENERO}
	 	 </if>
	 	 
	 	  <if test="NOMBRE_COMPLETO != null">
	 		and UPPER(persona.nombre_completo) like '%${NOMBRE_COMPLETO}%'
	 	 </if>
	 	 
	 	 <if test="TIPO_DOCUMENTO != null">
	 	 	and persona.tipo_documento=${TIPO_DOCUMENTO}
	 	 </if>
	 	 
	 	   <if test="NRO_DOCUMENTO != null">
	 	 	and  persona.nro_documento='${NRO_DOCUMENTO}'
	 	 </if>
	 	 
	 	 
	 	 	order by 1 asc
	 	 	
	 	 <if test="INICIO_PAG != null">
	 	   LIMIT 10 OFFSET ${INICIO_PAG};
	 	 </if>
  
   </select>
   
