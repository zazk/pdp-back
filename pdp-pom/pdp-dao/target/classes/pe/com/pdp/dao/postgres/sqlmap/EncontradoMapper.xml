     <select id="getEncontradoVigencia" resultType="HashMap" parameterType="HashMap" >
     		SELECT
			  id,
			  COALESCE("indVigencia", 1) AS ind_vigencia,
			  vigencia AS fec_vigencia,
			  fecha_encontrado as fecha_publicacion,
			  (
			  (DATE_PART('year', current_timestamp) - DATE_PART('year', COALESCE(vigencia, fecha_encontrado))) * 12 +
			  (DATE_PART('month', current_timestamp) - DATE_PART('month', COALESCE(vigencia, fecha_encontrado)))
			  ) AS diff
			FROM "pataPerro".tm_encontrado
			WHERE estado = 1
			AND COALESCE("indVigencia", 1) = 1
			AND usuario_registro = '${idUsuario}'
			AND 
			(
			  (DATE_PART('year', current_timestamp) - DATE_PART('year', COALESCE(vigencia, fecha_encontrado))) * 12 +
			  (DATE_PART('month', current_timestamp) - DATE_PART('month', COALESCE(vigencia, fecha_encontrado)))
			 )  >= #{maxMes}
			ORDER BY COALESCE(vigencia, fecha_encontrado) DESC
			LIMIT 1
     </select>
  
