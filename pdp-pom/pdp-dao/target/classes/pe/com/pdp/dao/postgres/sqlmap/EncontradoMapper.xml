  <select id="selectByEncontreMascota" parameterType="HashMap" resultMap="ResultEncontreMascota">
  
	  SELECT 
  	
  		 COALESCE(mascota.nombre, 'Desconocido') as nombre_mascota,
  		 mascota.genero, 
         genero.des_abreviacion as des_genero,
      	 mascota.tamanio,
         tamanio.des_nombre as des_tamanio,
      	 mascota.raza,
         raza.des_nombre as des_raza,
      	 mascota.distrito as distritoMascota, 
      	 mascota.caracteristica,
      	 mascota.ind_recibido, 
      	 mascota.des_comentario, 
         mascota.ubicacion_catastro as ubicacion_catastro_mascota,
         mascota.color,
         color.des_nombre as des_color, 
         mascota.estado as estado_mascota,
         encontre_mascota.*,
         ubi.nombre as desDistrito 
  		      
		 FROM "pataPerro".tm_mascota mascota
		  
          inner join "pataPerro".tm_encontrado encontre_mascota on
          encontre_mascota.mascota_id=mascota.id
          
          left join  "pataPerro".tm_elemento genero on
          genero.id=mascota.genero
          
          inner join  "pataPerro".tm_elemento tamanio on
          tamanio.id=mascota.tamanio
          
          left join  "pataPerro".tm_elemento raza on
          raza.id=mascota.raza
          
          inner join  "pataPerro".tm_elemento color on
          color.id=mascota.color
          
          
          left join "pataPerro".tm_ubigeo ubi on 
          ubi.id = encontre_mascota.distrito
          
	     where
		mascota.estado=1 and encontre_mascota.estado=1
	     
	    <if test="MASCOTA_ID != null">
	    	and mascota.id= ${MASCOTA_ID}
	 	 </if>
	 	 
	 	 <if test="MASCOTA_NOMBRE != null">
	 		and UPPER(mascota.nombre) like UPPER('%${MASCOTA_NOMBRE}%')
	 	 </if>
	 	 
	 	 <if test="TIPO_RAZA != null">
	 	 	and mascota.raza=${TIPO_RAZA}
	 	 </if>
	 	 
	 	  <if test="TIPO_TAMANIO != null">
	 	 	and mascota.tamanio=${TIPO_TAMANIO}
	 	 </if>
	 	 
	 	  <if test="TIPO_GENERO != null">
	 	 	and mascota.genero=${TIPO_GENERO}
	 	 </if>
	 	 
	 	   <if test="UBIGEO_DISTRITO != null">
	 	 	and encontre_mascota.distrito=${UBIGEO_DISTRITO}
	 	 </if>
	 	 
	 	 <if test="TIPO_COLOR != null">
	 	 	and mascota.color=${TIPO_COLOR}
	 	 </if>
	 	 
	 	  <if test="ENCONTRADO_MASCOTA_ID != null">
	    	and encontre_mascota.id= ${ENCONTRADO_MASCOTA_ID}
	 	 </if>
	 	 
	 	  <if test="ENCONTRADO_USUARIO_REGISTRO != null">
   	 		and encontre_mascota.usuario_registro = '${ENCONTRADO_USUARIO_REGISTRO}'
