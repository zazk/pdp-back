    		notificacion.usuario_destino,
     		notificacion.usuario_destino_id,
     		notificacion.fecha_recepcion,
     		notificacion.fecha_emision,
      		notificacion.estado,
  		    elemento1.des_nombre as des_tipo_medio,
  		    elemento2.des_nombre as des_tipo_notificacion,
  		    COALESCE(notificacion."ind_leido", 0) as ind_leido,
  		    notificacion.id_relacion,
  		    notificacion."desParametro1",
  		    notificacion."desParametro2"
  		    
		  FROM "pataPerro".tm_notificacion notificacion
		  inner join "pataPerro".tm_elemento elemento1 on
		  elemento1.id=notificacion.tipo_medio
		  inner join "pataPerro".tm_elemento elemento2 on
		  elemento2.id=notificacion.tipo_notificacion
	       
