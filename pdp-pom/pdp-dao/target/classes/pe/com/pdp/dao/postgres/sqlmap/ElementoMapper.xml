      <!-- QUERIES ADICIONALES -->
  	<select id="selectByElemento"  resultMap="ResultElemento" parameterType="HashMap">
  
	  	SELECT 
  		     
  			ele.id, 
  			ele.grupo_elemento_id, 
  			CASE WHEN ele.des_abreviacion!='' THEN ele.des_abreviacion ELSE '--'END AS des_abreviacion, 
      		ele.des_codigo, 
      		ele.des_nombre, 
      		ele.estado,
      		grupo.nombre as des_tipo_grupo
  		    
		  FROM "pataPerro".tm_elemento ele
		  inner join "pataPerro".tm_grupo_elemento grupo on
		  grupo.id=ele.grupo_elemento_id
	       
	     where
	
	     ele.estado=1 
	     
	    <if test="ELEMENTO_ID != null">
	    	and ele.id= ${ELEMENTO_ID}
	 	 </if>
	 	 
	 	 <if test="NOMBRE != null">
	 		and UPPER(ele.des_nombre) like '%${NOMBRE}%'
	 	 </if>
	 	 
	 	  <if test="TIPO_GRUPO != null">
	    	and ele.grupo_elemento_id= ${TIPO_GRUPO}
	 	 </if>
	 	 

	 	 	order by 1 asc
	 	 	
	 	 <if test="INICIO_PAG != null">
	 	   LIMIT 10 OFFSET ${INICIO_PAG};
	 	 </if>
  
   </select>
   
